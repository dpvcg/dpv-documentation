  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#PersonalDataHandling">
    <dct:description xml:lang="en">A high-level Class to describe 'data handling'. This can consist of personal data being processed for a purpose, involving entities, using technical and organisational measures, applicable risks, rights, and legal basis.</dct:description>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:label xml:lang="en">Personal Data Handling</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Axel Polleres</dct:creator>
