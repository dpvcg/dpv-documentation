<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
   xmlns:dct="http://purl.org/dc/terms/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:sw="http://www.w3.org/2003/06/sw-vocab-status/ns#"
>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasWithdrawalByJustification">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:label xml:lang="en">has withdrawal by justification</rdfs:label>
    <dct:description xml:lang="en">Specifies the justification for entity withdrawing consent</dct:description>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <rdfs:comment xml:lang="en">This field can be used to proivde a justification why the weithdrawal was done by another DataSubject or legal entity, e.g. declariing the relationship (parent, guardian), in combination with the field withdrawalBy</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasProvisionTime">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:description xml:lang="en">Specifies the instant in time when consent was given</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#Instant"/>
    <rdfs:label xml:lang="en">has provision time</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasExpiry">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <dct:description xml:lang="en">Generic property specifying when or under which condition(s) the consent will expire</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">has expiry</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasWithdrawalBy">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <rdfs:label xml:lang="en">has withdrawal by</rdfs:label>
    <dct:description xml:lang="en">Specifies the entity that withdrew consent</dct:description>
    <rdfs:comment xml:lang="en">Normally this would be the dataSubject, but in some exceptional cases, the consent might be withdraawn on behalf by someone else, e.g. parents of minors.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <rdfs:range rdf:resource="http://www.w3.org/ns/dpv#LegalEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#Consent">
    <dct:description xml:lang="en">Consent of the data subject regarding processing of their personal data</dct:description>
    <rdfs:label xml:lang="en">Consent</rdfs:label>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_11/oj"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#isExplicit">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:label xml:lang="en">is explicit</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:description xml:lang="en">Specifies consent is 'explicit'</dct:description>
    <rdfs:comment xml:lang="en">The conditions for what is considered 'explicit consent' differ by norms and laws.</rdfs:comment>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasConsentNotice">
    <rdfs:label xml:lang="en">has consent notice</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:description xml:lang="en">Specifies the notice provided in context of consent</dct:description>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdfs:comment xml:lang="en">The actual notice that the Data Subject received to consent to, either a text or link to a document, which should be usable to decide whether the form or consent was compliant to legislation, e.g. documenting how the user has been informed about rights and implications (such as, right to data portability,right to recitffy, right to erasure, right to restrict processing, right to object, rights regarding automated decision making or profiling, processors, third parties, sub-processors, outside-EEA transfers, automated decision-making, or other necessary details of the privacy-policy). Can be TextOrDocumentOrURI.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasExpiryCondition">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdfs:label xml:lang="en">has expiry condition</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/dpv#expiry"/>
    <rdfs:comment xml:lang="en">Can be TextOrDocumentOrURI</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:description xml:lang="en">Specifies the condition or event that determines the expiry of consent</dct:description>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasWithdrawalTime">
    <dct:description xml:lang="en">Specifies the instant in time when consent was withdrawn</dct:description>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:label xml:lang="en">has withdrawal time</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#Instant"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasWithdrawalMethod">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <rdfs:comment xml:lang="en">Can be used to record information of how to withdraw consent, e.g. by a click to a form, in writing, etc., by logging into a system and confirm per email, or with some additional authentication, etc.</rdfs:comment>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:description xml:lang="en">Specifries the method by which consent can be/has been withdrawn</dct:description>
    <rdfs:label xml:lang="en">has withdrawal method</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasProvisionByJustification">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <rdfs:comment xml:lang="en">This field can be used to proivde a justification why the provision was provided by another DataSubject or legal entity,  e.g. declariing the relationship (parent, guardian), in combination with the field provisionBy</rdfs:comment>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <dct:description xml:lang="en">Specifies the justification for entity providing consent</dct:description>
    <rdfs:label xml:lang="en">has provision by justification</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasProvisionMethod">
    <dct:description xml:lang="en">Specifies the method by which consent was provisioned or provided</dct:description>
    <rdfs:label xml:lang="en">has provision method</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <rdfs:comment xml:lang="en">Can be used to record information of how consent was provided e.g. by a click to a form, in writing, etc., by logging into a system and confirm per email, or with some additional authentication, etc.</rdfs:comment>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasExpiryTime">
    <rdfs:label xml:lang="en">has expiry time</rdfs:label>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/dpv#expiry"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:description xml:lang="en">Specifies the expiry time or duration for consent</dct:description>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasProvisionBy">
    <dct:description xml:lang="en">Specifies the entity that provisioned or provided consent</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:comment xml:lang="en">Normally this would be the dataSubject, but in some exceptional cases, the consent might be given on behalf by someone else, e.g. parents of minors.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/ns/dpv#LegalEntity"/>
    <rdfs:label xml:lang="en">has provision by</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
</rdf:RDF>
